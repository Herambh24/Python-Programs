def emoji_converter(messgae):
    words = message.split(' ')
    emojis = {
        ":)": "ðŸ˜ƒ",
        ":(": "ðŸ˜­",
        ":/": "ðŸ™",
        ";)": "ðŸ˜‰",
        "*(": "ðŸ˜µ"
    }
    output = ""
    for word in words:
        output += emojis.get(word, word) + " "
    return output

message = input(">")
print(emoji_converter(message))
